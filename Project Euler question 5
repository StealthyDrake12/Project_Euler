#function

def smallest_multiple(limit):
    
    #starting numbers

    number = 232792550

    while True:  


        divisible_by_all = True
        for divider in range(1, limit + 1):

            if number % divider != 0:
                divisible_by_all = False

                #answer not found

                break  
                
        if divisible_by_all:
            print (number)
            return number
        
        #add one
        number += 1
        print (number)



print(smallest_multiple(20))
print("answer found")   
